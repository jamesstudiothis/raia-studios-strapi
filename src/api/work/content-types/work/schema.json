{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Work",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "Creator": {
      "type": "string"
    },
    "Year": {
      "type": "string"
    },
    "workURL": {
      "type": "string",
      "required": false
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "About": {
      "type": "text"
    },
    "Credit": {
      "displayName": "Credits",
      "type": "component",
      "repeatable": true,
      "component": "layout.credits"
    },
    "capabilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::capability.capability"
    },
    "Tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "SEO": {
      "displayName": "SEO Information",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information"
    }
  }
}
