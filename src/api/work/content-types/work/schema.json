{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Projects",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "creator": {
      "type": "string"
    },
    "year": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "workUrl": {
      "type": "component",
      "repeatable": false,
      "component": "layout.video"
    },
    "about": {
      "type": "text"
    },
    "credit": {
      "displayName": "Credits",
      "type": "component",
      "repeatable": true,
      "component": "layout.credits"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "SEO": {
      "displayName": "SEO Information",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information"
    }
  }
}
